            <string>player_distance = player.x - x;
is_active = on_screen();
if (image_index == 2){
    image_index = 0;
}
if (is_active){
    if (!pause_frames){
        if (abs(player_distance) &lt;= ideal_distance){
            is_attacking = true;
        }
        else if (is_attacking){
                        //hsp = 0;
            dir = 0;
            if (image_index &lt; 1){
                image_index = 1;
                pause_frames = 40;
            }
            else {
                image_index = 2;
                instance_create(x, y, obj_sword_hbox);
                pause_frames = 30;
                is_attacking = false;
            }
        }
        else if (abs(player_distance) != ideal_distance &amp;&amp; image_index != 1){
                image_index = 0;
                if (abs(player_distance) &lt; ideal_distance){
                    dir = -sign(player_distance);
                }
                else {
                    dir = sign(player_distance);
                }
        }

        hsp = dir * move_speed;
        if (place_meeting(x + hsp, y, floorbox)){
            while(!place_meeting(x + sign(hsp), y, floorbox)){
                x += sign(hsp);
            }
            hsp = 0;
        }
        x += hsp;
    
    }
    else {
        pause_frames--;
    }
    
    if (image_index != 0){
        if (!invuln_frames){
            if (place_meeting(x, y, whipper) || place_meeting(x, y, obj_e_whipper)){
                script_knight_damage();
            }
        }
    }
}
else {
    image_index = 0;
    dir = 0;
    hsp = 0;
}
