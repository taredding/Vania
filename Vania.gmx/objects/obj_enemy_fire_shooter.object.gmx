            else {
                state = 2;
                move_frames = max_move_frames;
            }
            break;
        }
        
        case 2:{
            image_index = 0;
            if (move_frames){
                if (abs(player_distance) &lt; activate_distance){
                    dir = -sign(player_distance);
                }
                else if (abs(player_distance) == activate_distance){
                    dir = 0;
                }
                else {
