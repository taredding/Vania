    case 1:{
        player_xdistance = player.x - x; 
        vulnerable = true;

        if (!player.midair &amp;&amp; player.invuln_frames &lt; 20 || player.y &gt; y &amp;&amp; player.invuln_frames &lt; player.invuln_wait / 20 || dir == 0){
