            <string>///handle collision

if (!place_meeting(x + hsp, y + 2, floorbox) &amp;&amp; place_meeting(x + hsp, y + 3, obj_stairs)){
    y += 2;
}

if (place_meeting(x + hsp, y, floorbox)){
    y_plus = 0;
    if (!player.midair){
        while ((place_meeting(x + hsp, y - y_plus, floorbox) &amp;&amp; abs(y_plus) &lt;= 2)){
            y_plus += sign(hsp);
        }
    }
    if (place_meeting(x + hsp, y-y_plus, floorbox)){
        while(!place_meeting(x + sign(hsp), y, floorbox)){
            x += sign(hsp);
        }
        hsp = 0;
    }
    y -= y_plus;

}


if (vsp &lt; 10){
    vsp += grav;
}

//horizontal collision
if (place_meeting(x + hsp, y, floorbox)){
    while(!place_meeting(x + sign(hsp), y - 1, floorbox)){
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp; 
//vertical collision
if (place_meeting(x, y + vsp, floorbox)){
    while(!place_meeting(x, y + vsp, floorbox)){
        y += sign(vsp);
    }
    vsp = 0;
}

y += vsp;
